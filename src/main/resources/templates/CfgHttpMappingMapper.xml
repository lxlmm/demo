<mapper namespace="com.lxl.mapper.CfgHttpMappingMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.lxl.entity.CfgHttpMapping">
        <result column="mapping_id" property="mappingId" />
        <result column="cfg_http_code" property="cfgHttpCode" />
